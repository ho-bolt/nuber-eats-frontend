{
  "name": "nuber-eats-frontend",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@apollo/client": "^3.8.8",
    "@fortawesome/fontawesome-svg-core": "^6.5.1",
    "@fortawesome/free-brands-svg-icons": "^6.5.1",
    "@fortawesome/free-regular-svg-icons": "^6.5.1",
    "@fortawesome/free-solid-svg-icons": "^6.5.1",
    "@fortawesome/react-fontawesome": "^0.2.0",
    "@testing-library/jest-dom": "^5.17.0",
    "@testing-library/react": "^13.4.0",
    "@testing-library/user-event": "^13.5.0",
    "@types/jest": "^27.5.2",
    "@types/node": "^16.18.68",
    "@types/react": "^18.2.45",
    "@types/react-dom": "^18.2.18",
    "@types/react-router-dom": "^5.3.3",
    "apollo": "^2.34.0",
    "graphql": "^15.8.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-helmet": "^6.1.0",
    "react-helmet-async": "^2.0.4",
    "react-hook-form": "^7.49.2",
    "react-router-dom": "^5.3.4",
    "react-scripts": "5.0.1",
    "rimraf": "^5.0.5",
    "tailwindcss": "^3.4.0",
    "typescript": "^4.9.5",
    "web-vitals": "^2.1.4"
  },
  "scripts": {
    "tailwind:build": "tailwind build -i ./src/styles/tailwind.css -o ./src/styles/style.css",
    "start": "npm run compile & npm run tailwind:build & react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "compile": "rimraf src/__generated__ &&  graphql-codegen",
    "watch": "graphql-codegen -w"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@graphql-codegen/cli": "^5.0.0",
    "@graphql-codegen/client-preset": "^4.1.0",
    "@graphql-codegen/typescript": "^4.0.1",
    "@graphql-codegen/typescript-operations": "^4.0.1",
    "@types/react-helmet-async": "^1.0.3",
    "autoprefixer": "^10.4.16",
    "postcss": "^8.4.32",
    "ts-node": "^10.9.2"
  },
  "overrides": {
    "@apollo/client": {
      "react": "$react"
    },
    "@apollographql/graphql-language-service-interface": {
      "graphql": "$graphql"
    },
    "@apollographql/graphql-language-service-parser": {
      "graphql": "$graphql"
    },
    "@apollographql/graphql-language-service-types": {
      "graphql": "$graphql"
    },
    "@apollographql/graphql-language-service-utils": {
      "graphql": "$graphql"
    }
  }
}
